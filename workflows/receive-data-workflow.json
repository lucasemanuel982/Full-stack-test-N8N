        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-receive",
      "name": "Webhook - Receive Data",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [240, 300],
      "webhookId": "receive-data-webhook"
    },
    {
      "parameters": {
        "operation": "insert",
        "table": "users",
        "columns": "nome, email, phone",
        "additionalFields": {}
      },
      "id": "postgres-insert",
      "name": "PostgreSQL - Insert Data",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.4,
      "position": [460, 300],
      "credentials": {
        "postgres": {
          "id": "postgres-credentials",
          "name": "PostgreSQL Neon"
        }
      }
    },
    {
      "parameters": {
        "operation": "select",
        "table": "users",
        "additionalFields": {}
      },
      "id": "postgres-select",
      "name": "PostgreSQL - Select All Users",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.4,
      "position": [680, 300],
      "credentials": {
        "postgres": {
          "id": "postgres-credentials",
          "name": "PostgreSQL Neon"
        }
      }
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ $json }}"
      },
      "id": "respond-success",
      "name": "Respond with Data",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [900, 300]
    }
  ],
  "connections": {
    "Webhook - Receive Data": {
      "main": [
        [
          {
            "node": "PostgreSQL - Insert Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "PostgreSQL - Insert Data": {
      "main": [
        [
          {
            "node": "PostgreSQL - Select All Users",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "PostgreSQL - Select All Users": {
      "main": [
        [
          {
            "node": "Respond with Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {},
  "versionId": "1"
